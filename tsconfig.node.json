{
	"compilerOptions": {
		// "composite": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
		"target": "ES2022",
		"lib": [
			"ES2023"
		],
		"skipLibCheck": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		// "allowSyntheticDefaultImports": true,
		"isolatedModules": true,
		"moduleDetection": "force",
		"noEmit": true,
		"strict": true
	},
	"include": [
		"vite.config.ts"
	]
}
